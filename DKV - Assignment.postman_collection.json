	"item": [
		{
			"name": "Get Request - Positive Test - Valid PetId",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Check Status Code is 200\", () => pm.response.to.have.status(200));\r",
							"pm.test(\"Check Content-Type is application/json\", () => pm.response.to.have.header(\"content-type\"));\r",
							"const json = pm.response.json();\r",
							"pm.test(\"Check response json has category field as a non-empty object\", () => pm.expect(json.category).to.be.a(\"object\").and.not.empty);\r",
							"pm.test(\"Check response json has name field as a non-empty string\", () => pm.expect(json.name).to.be.a(\"string\").and.not.empty);\r",
							"pm.test(\"Check response json has photoUrls field as a non-empty object\", () => pm.expect(json.photoUrls).to.be.a(\"array\").and.not.empty);\r",
							"pm.test(\"Check response json has tags field as a non-empty object\", () => pm.expect(json.tags).to.be.a(\"array\").and.not.empty);\r",
							"pm.test(\"Check response json has status field as one of the following - available, pending or sold\", () => pm.expect([\"available\",\"pending\",\"sold\"]).to.include(json.status));"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
